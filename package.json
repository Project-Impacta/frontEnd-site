{
  "name": "store_impacta-front_end",
  "description": "Este é um projeto acadêmico desenvolvido para fins de aprendizado na faculdade. Todos os processos, links e imagens são parte de um cenário simulado e podem não refletir uma aplicação comercial real.",
  "version": "4.5.0",
  "author": "Jh0w",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/Project-Impacta"
  },
  "private": true,
  "release": {
    "branches": [
      "main",
      "dev"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "build.zip",
              "label": "Build for version ${nextRelease.version}"
            },
            {
              "path": "coverage.zip",
              "label": "Coverage report for version ${nextRelease.version}"
            }
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/github": "^9.2.6",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "file-loader": "^6.2.0",
    "prettier": "^3.2.5",
    "read-package-up": "^11.0.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.14",
    "@telefonica/mistica": "^15.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "jwt-decode": "^4.0.0",
    "next": "14.1.0",
    "next-auth": "^4.24.7",
    "next-themes": "^0.2.1",
    "path": "^0.12.7",
    "postcss": "^8.4.38",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.3",
    "tailwindcss": "^3.4.3",
    "yarn": "^2.4.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "node format.js",
    "export": "next export",
    "release:dryrun": "semantic-release --dry-run",
    "typecheck": "tsc --project tsconfig.json"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=10.3.0"
  }
}
